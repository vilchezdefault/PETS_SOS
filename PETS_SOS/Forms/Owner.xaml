            </Border.Background>
           
        </Border>
        <Label x:Name="lblFirstName" Content="First Name" HorizontalAlignment="Left" Margin="10,20,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.526,0.361"/>
