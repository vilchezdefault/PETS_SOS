        <TextBox x:Name="txtLastName" TextWrapping="Wrap" Text="" Margin="10,169,464,245"/>
        <Button x:Name="btnMainWindow" Content="Main Window" Height="25" Margin="10,382,563,43" Click="btnMainWindow_Click"/>
        <Button x:Name="BtnSave" Content="Save" Height="25" Margin="286,382,287,43"/>
       


        <Image Source="/IMGS/logo juan.png" Margin="343,187,10,84" />
        <Label x:Name="lblTitle" Content="Owners Window" Margin="400,151,68,269" HorizontalContentAlignment="Center" UseLayoutRounding="False" FontFamily="Segoe UI Black" FontSize="16" FontWeight="Bold"/>

    </Grid>
