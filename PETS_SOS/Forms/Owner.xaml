        <Label x:Name="lblId" Content="ID" HorizontalAlignment="Left" Margin="10,274,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.526,0.361"/>
        <Label x:Name="lblEmail"  Content="Email" HorizontalAlignment="Left" Margin="390,34,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.526,0.361"/>
        <Label x:Name="lblPhone" Content="Phone" HorizontalAlignment="Left" Margin="390,99,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.526,0.361"/>
        <TextBox x:Name="txtsecondName" TextWrapping="Wrap" Text="" Margin="10,104,464,310" AutomationProperties.Name="txtSecondName"/>
        <TextBox x:Name="txtFirstName" TextWrapping="Wrap" Text="" Margin="10,46,464,368" AutomationProperties.Name="txtFirstName"/>
